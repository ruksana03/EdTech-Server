{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/files/(.*)",
      "dest": "/files/$1",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "src/app.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }
  ]
}

